Func calcTemp(Celsius: Double)->
(Double,Double){
    var fahrenheit: Double
var kelvin: Double
    fahrenheit = (celcius * 9 / 5) + 32
    kelvin = celsius + 273.15
    return Fahrenheit,kelvin

}

