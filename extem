//Assignment 1

var firstName = fName.text!
        
        // read the last name and assign it to the variable
        var lastName = lName.text!
        
        // read the birth year and assign it to the variable
        
        var birthYear = bYear.text!
        
        //to calculate the age based on given birth year and current year
        
        let date = Date()
        let dateFormatter = DateFormatter()
        dateFormatter.dateFormat = "yyyy"
        let yearString = dateFormatter.string(from: date)
        
        var y1 :Int = Int(birthYear) ?? 0

        var y2 :Int = Int(yearString) ?? 0
        var age :Int = y2-y1
        // String interpolation to display
        
        var output1 = "Full Name : \(lastName) \(firstName)"
        var output2 = "Initials : \(lastName.first!)  \(firstName.first!)"
        var output3 = "Age : \(age)"
        displayOutletLabel.text = "Details\r\r\(output1)\r\r\(output2)\r\r\(output3)"
    }
    
    
    @IBAction func resetClicked(_ sender: UIButton) {
        displayOutletLabel.text = ""
    }

}


